<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Carrito" table="carrito">
    <unique-constraints>
      <unique-constraint name="carrito_pk" columns="id_carrito"/>
    </unique-constraints>
    <id name="idCarrito" type="integer" column="id_carrito">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="idUsu" type="string" column="id_usu" length="50" nullable="true"/>
    <field name="idProduct" type="string" column="id_product" length="50" nullable="true"/>
    <field name="producto" type="string" column="producto" length="50" nullable="true"/>
    <field name="precio" type="decimal" column="precio" precision="10" scale="2" nullable="true"/>
    <field name="cantidad" type="integer" column="cantidad" nullable="true"/>
    <field name="total" type="decimal" column="total" precision="10" scale="2" nullable="true"/>
  </entity>
</doctrine-mapping>
